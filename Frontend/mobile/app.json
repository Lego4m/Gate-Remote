{
  "expo": {
    "name": "Gate Remote",
    "slug": "GateRemote",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "primaryColor": "#191919",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#191919"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.lego.gateremote",
      "versionCode": 2,
      "adaptiveIcon": {
        "backgroundColor": "#191919"
      },
      "permissions": [
        "VIBRATE"
      ]
    }
  }
}
